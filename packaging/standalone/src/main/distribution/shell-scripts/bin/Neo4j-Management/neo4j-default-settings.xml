    <setting file="neo4j-server.properties" name="org.neo4j.server.webserver.address">127.0.0.1</setting>
    <setting file="neo4j-server.properties" name="dbms.security.auth_enabled">true</setting>
    <setting file="neo4j-server.properties" name="org.neo4j.server.webserver.port">7474</setting>
    <setting file="neo4j-server.properties" name="org.neo4j.server.webserver.https.enabled">true</setting>
